{
  "name": "boilerplate-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development babel-node server/index.js",
    "build": "NODE_ENV=production babel-node server/index.js",
    "test": "jest",
    "lint": "eslint server/ client/",
    "debug": "NODE_ENV=development node_modules/.bin/nodemon node_modules/babel-cli/bin/babel-node.js --debug --presets es2015 -- server/index.js --debug"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "expect": "^1.20.2",
    "express": "~4.14.0",
    "helmet": "^3.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.26",
    "morgan": "^1.8.2",
    "swagger-ui-express": "^1.0.7",
    "winston": "^2.3.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "xlsx": "^0.10.3",
    "webpack": "^2.5.1",
    "html-webpack-plugin": "^2.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "webpack-md5-hash": "0.0.5",
    "connect-history-api-fallback": "^1.3.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "axios": "^0.16.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chakram": "^1.5.0",
    "css-loader": "^0.28.1",
    "directory-named-webpack-plugin": "^2.1.0",
    "enzyme": "^2.8.2",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "express": "^4.15.2",
    "file-loader": "0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "1.0.0-alpha.2",
    "jest": "^20.0.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "2.3.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "rules": {
      "quotes": 0,
      "no-console": 1,
      "no-debugger": 1,
      "no-var": 1,
      "semi": [
        1,
        "always"
      ],
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0,
      "jsx-quotes": 1,
      "react/display-name": [
        1,
        {
          "ignoreTranspilerName": false
        }
      ],
      "react/forbid-prop-types": [
        0,
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-curly-spacing": 1,
      "react/jsx-indent-props": 0,
      "react/jsx-key": 1,
      "react/jsx-max-props-per-line": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-no-duplicate-props": 1,
      "react/jsx-no-literals": 0,
      "react/jsx-no-undef": 1,
      "react/jsx-pascal-case": 1,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/jsx-wrap-multilines": 1,
      "react/no-danger": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 1,
      "react/no-multi-comp": 0,
      "react/no-set-state": 0,
      "react/no-unknown-property": 1,
      "react/prefer-es6-class": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "import/extensions": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1
    },
    "globals": {}
  }
}
